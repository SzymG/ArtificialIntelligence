package com.sample
 
import com.sample.DroolsTest.Message;
import com.sample.DroolsTest.Question;

rule "First"
	when 
		q : Question( content != null )
	then
		System.out.println("Siema! Pierwsze pytanie");
		q.setGUI(q.getContent(),q.getAnswers());
end
 
